{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": ["./base.json", "@tsconfig/svelte/tsconfig.json"],
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "isolatedModules": true
  }
}
